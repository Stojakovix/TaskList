<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TaskList.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TaskList"
    xmlns:views="clr-namespace:TaskList.Views"
    Shell.FlyoutBehavior="Flyout"
    Shell.NavBarIsVisible="False">



    <ShellContent
        Title="MainItemPage"
        ContentTemplate="{DataTemplate views:MainItemPage}"
        Route="MainItemPage"/>
    <ShellContent
        Title="EntryItem"
        ContentTemplate="{DataTemplate views:EntryItem}"
        Route="EntryItem"/>



</Shell>
